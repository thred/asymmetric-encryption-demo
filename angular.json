{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "asymmetric-encryption-demo": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:resolver": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "i18n": {
                "sourceLocale": "de",
                "locales": {
                    "en": {
                        "translation": "src/locale/messages.en.xlf"
                    }
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/asymmetric-encryption-demo",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": ["@angular/localize/init"],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            }
                        ],
                        "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "baseHref": "https://thred.github.io/asymmetric-encryption-demo/",
                            "localize": ["de", "en"],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "localize": false,
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        },
                        "en": {
                            "localize": ["en"],
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "asymmetric-encryption-demo:build:production"
                        },
                        "development": {
                            "buildTarget": "asymmetric-encryption-demo:build:development"
                        },
                        "en": {
                            "buildTarget": "asymmetric-encryption-demo:build:en"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": ["@angular/localize/init"],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            }
                        ],
                        "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "src/styles.scss"],
                        "scripts": []
                    }
                }
            }
        }
    }
}
